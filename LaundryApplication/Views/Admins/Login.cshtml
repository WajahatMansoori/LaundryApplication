@model LaundryApplication.Models.Admin

@{
    ViewBag.Title = "Login";
    //Layout = "~/Views/Shared/_LayoutBEnd.cshtml";
    Layout = null;
}

<head>
    <meta charset="UTF-8">
    <title>CodePen - Panda Login</title>
    <link rel="stylesheet" href="~/Panda/css/style.css">
</head>

@Html.Raw(TempData["AdminfailLogin"])

<div class="panda">
    <div class="ear"></div>
    <div class="face">
        <div class="eye-shade"></div>
        <div class="eye-white">
            <div class="eye-ball"></div>
        </div>
        <div class="eye-shade rgt"></div>
        <div class="eye-white rgt">
            <div class="eye-ball"></div>
        </div>
        <div class="nose"></div>
        <div class="mouth"></div>
    </div>
    <div class="body"> </div>
    <div class="foot">
        <div class="finger"></div>
    </div>
    <div class="foot rgt">
        <div class="finger"></div>
    </div>
</div>

@using (Html.BeginForm())
{
    <div class="hand"></div>
    <div class="hand rgt"></div>
    <h1>Admin Login</h1>
    <div class="form-group">
        @Html.TextBoxFor(model => model.UserName, new { @class = "form-control" })
        @*<input required="required" class="form-control" />*@
        <label class="form-label">Username    </label>
    </div>
    <div class="form-group">
        @Html.PasswordFor(model => model.Password, new { @class = "form-control" })
        @*<input id="password"  type="password" required="required" class="form-control" />*@

        <label class="form-label">Password</label>


        <button class="btn">Login </button>
    </div>
}

<script src='https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
<script src="~/Panda/Js/script.js"></script>

