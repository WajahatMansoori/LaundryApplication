@model IEnumerable<LaundryApplication.Models.OnlineOrder>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutBEnd.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cat_Des)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pay_Des)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Item_Quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Amount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pay_Status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Category.Cat_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Customer.Cus_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PaymentMethod.Pay_Type)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Cat_Des)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pay_Des)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Item_Quantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Amount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pay_Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Category.Cat_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Customer.Cus_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PaymentMethod.Pay_Type)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Ord_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Ord_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Ord_Id }) |
            @Html.ActionLink("Generate Invoice", "BarcodeGenerate", "OnlineOrders", new { id = item.Ord_Id, }, new { @class = "btn btn-danger" })
        </td>
    </tr>
}

</table>
